{
   "compilerOptions": {
     "composite": false,
     "declaration": true,
     "declarationMap": true,
     "esModuleInterop": true,
     "forceConsistentCasingInFileNames": true,
     "inlineSources": false,
     "isolatedModules": true,
     "moduleResolution": "node",
     "noUnusedLocals": false,
     "noUnusedParameters": false,
     "preserveWatchOutput": true,
     "skipLibCheck": true,
     "strict": true,
     "jsx": "react-jsx",
     "lib": ["dom", "ES2019"],
     "module": "ESNext",
     "target": "ES2019"
   },
   "exclude": ["node_modules"]
 }
 